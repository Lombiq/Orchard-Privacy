@inject OrchardCore.Settings.ISiteService _siteService

@{
    var registrationConsentText = (await _siteService.GetSiteSettingsAsync())
        .As<ContentItem>(TypeNames.PrivacyRegistrationConsentSettings).As<LiquidPart>()?.Liquid;
}

@await Orchard.LiquidToHtmlAsync(registrationConsentText)
