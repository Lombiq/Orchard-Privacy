@model RegistrationConsentCheckboxViewModel

<fieldset class="form-group" asp-validation-class-for="RegistrationCheckbox">
    <div class="col-md-9">
        <div class="form-check">
            <input asp-for="RegistrationCheckbox" class="form-check-input" required />
            <label>
                <shape Type="Lombiq_Privacy_RegistrationCheckbox_Text" />
            </label><br>
            <span asp-validation-for="RegistrationCheckbox" class="text-danger"></span>
        </div>
    </div>
</fieldset>
